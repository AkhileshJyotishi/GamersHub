{
  "name": "gch-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "update": "yarn upgrade-interactive --latest",
    "lint": "next lint",
    "lint:eslint": "eslint . --fix --ext ts --ext tsx --ext js",
